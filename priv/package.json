{
  "name": "xprof",
  "version": "1.1.0",
  "description": "XProf - BEAM profiler",
  "repository": {
    "type": "git",
    "url": "https://github.com/appliscale/xprof.git"
  },
  "keywords": [
    "xprof"
  ],
  "authors": [
    "Michal Niec <michal.niec@appliscale.io>"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/appliscale/xprof/issues"
  },
  "homepage": "https://github.com/appliscale/xprof",
  "devDependencies": {
    "babel-core": "6.2.1",
    "babel-loader": "6.2.0",
    "babel-preset-airbnb": "2.1.1",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-register": "6.18.0",
    "bower": "1.8.0",
    "chai": "3.5.0",
    "css-loader": "0.23.0",
    "eslint": "3.12.2",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-json": "1.2.0",
    "eslint-plugin-react": "6.8.0",
    "file-loader": "0.8.5",
    "jsdom": "9.9.1",
    "mocha": "3.2.0",
    "style-loader": "0.13.0",
    "url-loader": "0.5.7",
    "webpack": "1.12.9"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": [
      "import",
      "react",
      "json"
    ],
    "rules": {
      "eqeqeq": "error",
      "curly": "error",
      "indent": [
        "error",
        2
      ],
      "quotes": [
        "error",
        "double"
      ]
    }
  }
}
